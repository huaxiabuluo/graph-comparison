{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "useBuiltIns": "usage",
      "corejs": {
        "version": 3,
        "proposals": true
      }
    }],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "react-hot-loader/babel",
    ["babel-plugin-import", { "libraryName": "antd", "style": "css" }]
  ]
}